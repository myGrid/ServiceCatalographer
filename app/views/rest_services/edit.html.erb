<h1>Editing rest_service</h1>

<%= form_for(@rest_service) do |f| %>
    <% if !@rest_service.nil? && !@rest_service.errors.nil? then %>
        <% @rest_service.errors.full_messages.each do |err| %>
            <%= err %>
        <% end %>
    <% end %>

  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @rest_service %> |
<%= link_to 'Back', rest_services_path %>
